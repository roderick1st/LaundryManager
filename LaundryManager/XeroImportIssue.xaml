<Window x:Class="LaundryManager.XeroImportIssue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LaundryManager"
        mc:Ignorable="d"
        Title="XeroImportIssue" Height="450" Width="800"
        Loaded="XeroImportIssue_Window_Loaded">
    <Grid>
        <Label Content="Xero Ticket Import Report" FontSize="25"/>
        <DataGrid x:Name="dataGridReport" Height="290" Width="770" Margin="0 45 0 0" HorizontalAlignment="Center" HorizontalScrollBarVisibility="Auto"
                  VerticalScrollBarVisibility="Auto" VerticalAlignment="Top" d:ItemsSource="{d:SampleData}">
        </DataGrid>
        <Button x:Name="btnOK" Content="OK" Click="btnOK_Click" FontSize="25" Background="YellowGreen" Height="56" Width="150" VerticalAlignment="Bottom"
                Margin="15 0 0 10" HorizontalAlignment="Left"/>

    </Grid>
</Window>
